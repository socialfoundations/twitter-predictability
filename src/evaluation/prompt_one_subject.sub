executable = scripts/$(Model)/prompt_one.sh
arguments = --debug $(Args)

# hardware requirements
request_gpus = 2
request_cpus = 2
request_memory = 32768
request_disk = 200G   
requirements = CUDACapability >= 7.0
requirements = TARGET.CUDAGlobalMemoryMb > 50000

# other options
error = out/prompt_one.err
output = out/prompt_one.out
log = out/prompt_one.log

queue Args from args/$(Model)/nonfinetuned.txt
